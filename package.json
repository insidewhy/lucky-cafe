{
  "name": "lucky-cafe",
  "version": "0.11.0",
  "description": "typescript library for retrieving ordered interleaved pages of items from multiple asynchronous paginated sources",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "prepublish": "pnpm build",
    "lint-check": "eslint .",
    "lint": "pnpm lint-check --fix",
    "format-check": "prettier --check .",
    "format": "pnpm format-check --write",
    "type-check": "pnpm tsc --noEmit",
    "test": "pnpm vitest",
    "fix": "pnpm lint && pnpm format",
    "tag": "./scripts/tag.sh",
    "validate": "pnpm lint-check && pnpm format-check && pnpm type-check && pnpm test run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/insidewhy/lucky-cafe.git"
  },
  "keywords": [
    "pagination"
  ],
  "author": "insidewhy",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@eslint/compat": "^1.2.2",
    "@eslint/js": "^9.14.0",
    "@types/node": "22.14.1",
    "@typescript-eslint/parser": "^8.13.0",
    "eslint": "9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-import": "^2.31.0",
    "globals": "^16.0.0",
    "prettier": "3.5.3",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.12.2",
    "vitest": "^3.1.2"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@9.12.2",
  "volta": {
    "pnpm": "9.11.0",
    "node": "20.17.0"
  }
}
